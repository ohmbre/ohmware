/dts-v1/;
#include "../../../../arm/boot/dts/bcm2837.dtsi"
#include "../../../../arm/boot/dts/bcm2835-rpi.dtsi"
#include "../../../../arm/boot/dts/bcm283x-rpi-usb-host.dtsi"

#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/clock/bcm2835.h>
#include <dt-bindings/input/linux-event-codes.h>

#define ALT(x) BCM2835_FSEL_ALT ## x
#define DIR(x) BCM2835_FSEL_GPIO_ ## x
#define PULL(x) BCM2835_PUD_ ## x


/ {
	compatible = "raspberrypi,cm3", "brcm,bcm2837";
	model = "Raspberry Pi Compute Module 3";
	
	/delete-node/ chosen;
	
	memory {
		reg = <0 0x40000000>;
	};
	
	/delete-node/ leds;
	/delete-node/ cpus;
	
	expgpio: expgpio {
                compatible = "brcm,bcm2835-expgpio";
                gpio-controller;
		#gpio-cells = <2>;
		firmware = <&firmware>;
                status = "okay";
        };
	
	/*vchiq: vchiq {
		compatible = "brcm,bcm2835-vchiq";
		reg = <0x7e00b840 0xf>;
		interrupts = <0 2>;
		cache-line-size = <32>;
		firmware = <&firmware>;
        };*/
	
	
	cpus: cpus {
		#address-cells = <1>;
                #size-cells = <0>;
		
		v8_cpu0: cpu@0 {
			device_type = "cpu";
                        compatible = "arm,cortex-a53", "arm,armv8";
                        reg = <0>;
                        clock-frequency = <1200000000>;
		};
		
	        v8_cpu1: cpu@1 {
			device_type = "cpu";
                        compatible = "arm,cortex-a53", "arm,armv8";
                        reg = <1>;
                        clock-frequency = <1200000000>;
                        enable-method = "spin-table";
			cpu-release-addr = <0x0 0xe0>;
                };
		
                v8_cpu2: cpu@2 {
                        device_type = "cpu";
                        compatible = "arm,cortex-a53", "arm,armv8";
			reg = <2>;
                        clock-frequency = <1200000000>;
			enable-method = "spin-table";
                        cpu-release-addr = <0x0 0xe8>;
		};
		
                v8_cpu3: cpu@3 {
                        device_type = "cpu";
			compatible = "arm,cortex-a53", "arm,armv8";
                        reg = <3>;
                        clock-frequency = <1200000000>;
			enable-method = "spin-table";
                        cpu-release-addr = <0x0 0xf0>;
                };
	};
	
	soc {
		syscon@40000000 {
			compatible = "brcm,bcm2836-arm-local", "syscon";
			reg = <0x40000000 0x100>;
		};
		
		rotary@0 {
			compatible = "rotary-encoder";
			gpios = <&gpio 45 GPIO_ACTIVE_LOW>, <&gpio 44 GPIO_ACTIVE_LOW>;
			linux,axis = <0>;
			rotary-encoder,relative-axis;
			rotary-encoder,encoding = "gray";
			pinctrl-names = "default";
			pinctrl-0 = <&encoder_pins>;
		};
		
		input_gates {
			pinctrl-names = "default";
			pinctrl-0 = <&gatein_pins>;
		};
		
		gpio-keys {
			compatible = "gpio-keys";
			pinctrl-names = "default";
			pinctrl-0 = <&gateout_pins &gatein_pins>;
			
			gate-in-1 {
				label = "gate in 1";
				linux,code = <BTN_TRIGGER_HAPPY1>;
				gpios = <&gpio 3 GPIO_ACTIVE_HIGH>;
			};
			
			gate-in-2 {
				label = "gate in 2";
				linux,code = <BTN_TRIGGER_HAPPY2>;
				gpios = <&gpio 4 GPIO_ACTIVE_HIGH>;
			};
			
			gate-in-3 {
				label = "gate in 3";
				linux,code = <BTN_TRIGGER_HAPPY3>;
				gpios = <&gpio 5 GPIO_ACTIVE_HIGH>;
			};
			
			gate-in-4 {
				label = "gate in 4";
				linux,code = <BTN_TRIGGER_HAPPY4>;
				gpios = <&gpio 12 GPIO_ACTIVE_HIGH>;
			};
			
			gate-in-5 {
				label = "gate in 5";
				linux,code = <BTN_TRIGGER_HAPPY5>;
				gpios = <&gpio 13 GPIO_ACTIVE_HIGH>;
			};
			
			gate-in-6 {
				label = "gate in 6";
				linux,code = <BTN_TRIGGER_HAPPY6>;
				gpios = <&gpio 32 GPIO_ACTIVE_HIGH>;
			};

			click {
				label = "encoder button";
				linux,code = <BTN_SELECT>;
				gpios = <&gpio 40 GPIO_ACTIVE_LOW>;
				pinctrl-names = "default";
				pinctrl-0 = <&button_pin>;
			};
		};
		
		/delete-node/ sdhci@7e300000;
		wifi_pwrseq: wifi_pwrseq {
			compatible = "mmc-pwrseq-emmc";
			reset-gpios = <&gpio 33 GPIO_ACTIVE_LOW>;
		};
		
		sdhci: sdhci@7e300000 {
			compatible = "brcm,bcm2835-sdhci";
			pinctrl-names = "default";
			pinctrl-0 = <&sdio_pins>;
			reg = <0x7e300000 0x100>;
			interrupts = <2 30>;
			clocks = <&clocks BCM2835_CLOCK_EMMC>;
			bus-width = <4>;
			max-frequency = <50000000>;
			mmc-pwrseq = <&wifi_pwrseq>;
			status = "okay";
			removable;
		};

	
		/delete-node/ mmc@7e202000;
		sdhost: mmc@7e202000 {
			compatible = "brcm,bcm2835-sdhost";
			reg = <0x7e202000 0x100>;
			interrupts = <2 24>;
			clocks = <&clocks BCM2835_CLOCK_VPU>;
			dmas = <&dma 13>;
			dma-names = "rx-tx";
			pinctrl-names = "default";
			pinctrl-0 = <&sdhost_pins>;
			status = "okay";
			bus-width = <4>;
			brcm,overclock-50 = <50>;
			brcm,pio-limit = <1>;
			non-removable;
		};

		sound: sound {
			compatible = "ohmbre,ohmbre-codec";
			i2s-controller = <&i2s>;
			status = "okay";
		};

		/delete-node/ pixelvalve@7e206000;
		/delete-node/ pixelvalve@7e207000;
		/delete-node/ pixelvalve@7e807000;
	
		pixelvalve0: pixelvalve@7e206000 {
			compatible = "brcm,bcm2835-pixelvalve0";
			reg = <0x7e206000 0x100>;
			interrupts = <2 13>;
			status = "okay";
		};
		
		pixelvalve1: pixelvalve@7e207000 {
			compatible = "brcm,bcm2835-pixelvalve1";
			reg = <0x7e207000 0x100>;
			interrupts = <2 14>;
			status = "okay";
		};
		
		pixelvalve2: pixelvalve@7e807000 {
			compatible = "brcm,bcm2835-pixelvalve2";
			reg = <0x7e807000 0x100>;
			interrupts = <2 10>;
			status = "okay";
		};

		/delete-node/ hvs@7e400000;
		hvs: hvs@7e400000 {
			compatible = "brcm,bcm2835-hvs";
			reg = <0x7e400000 0x6000>;
			interrupts = <2 1>;
			status = "okay";
		};
	};


};


&gpio {

        tft_spi_pins: tft_pins {
                brcm,pins = <9 10 11>;
                brcm,function = <ALT(0)>;
        };

	tft_cs_pin: tft_cs_pin {
		brcm,pins = <8>;
		brcm,function = <DIR(OUT)>;
	};

	tft_int_pin: tft_int_pin {
		brcm,pins = <7>;
		brcm,function = <DIR(IN)>;
		brcm,pull = <PULL(UP)>;
	};

        sdio_pins: sdio_pins {
		brcm,pins = <22 23 24 25 26 27>;
                brcm,function = <ALT(3)>;
                brcm,pull = <PULL(OFF) PULL(UP) PULL(UP) PULL(UP) PULL(UP) PULL(UP)>;
        };

	sdhost_pins: sdhost_pins {
		brcm,pins = <48 49 50 51 52 53>;
                brcm,function = <ALT(0)>;
                brcm,pull = <PULL(UP) PULL(UP) PULL(UP) PULL(UP) PULL(UP) PULL(UP)>;
        };

	blue_pins: blue_pins {
		brcm,pins = <14 15 16 17>;
		brcm,function = <ALT(0) ALT(0) ALT(3) ALT(3)>;
	};

	encoder_pins: encoder_pins {
		brcm,pins = <44 45>;
		brcm,function = <DIR(IN)>;
		brcm,pull = <PULL(UP)>;
	};

	button_pin: button_pin {
		brcm,pins = <40>;
		brcm,function = <DIR(IN)>;
		brcm,pull = <PULL(UP)>;
	};

	gatein_pins: gatein_pins {
		brcm,pins = <3 4 5 12 13 32>;
		brcm,function = <DIR(IN)>;
		brcm,pull = <PULL(UP)>;
	};
		
	gateout_pins: gateout_pins {
		brcm,pins = <39 38 37 36 35 34>;
		brcm,function = <DIR(OUT)>;
		brcm,pull = <PULL(UP)>;
	};

	codec_i2c_pins: codec_i2c_pins {
                brcm,pins = <0 1 2>;
		brcm,function = <ALT(0) ALT(0) DIR(OUT)>;
		brcm,pull = <PULL(UP) PULL(UP) PULL(UP)>;
	};

	codec_i2s_pins: codec_i2s_pins {
                brcm,pins = <28 29 30 31>;
       	        brcm,function = <ALT(2)>;
	};

	adc_spi_pins: adc_spi_pins {
		brcm,pins = <19 20 21>;
		brcm,function = <ALT(4)>;
	};

	adc_cs_pin: adc_cs_pin {
		brcm,pins = <18>;
		brcm,function = <DIR(OUT)>;
	};

	dac_spi_pins: dac_spi_pins {
		brcm,pins = <41 42>;
		brcm,function = <ALT(4)>;
	};

	dac_cs_pin: dac_cs_pin {
		brcm,pins = <43>;
		brcm,function = <DIR(OUT)>;
	};
};


&uart0 {
	pinctrl-names = "default";
	pinctrl-0 = <&blue_pins>;
	clock-frequency = <48000000>;
	uart-has-rtscts;
        status = "okay";
};

&uart1 {
	status = "disabled";
};

&spi { 

        pinctrl-names = "default";
	pinctrl-0 = <&tft_spi_pins &tft_cs_pin>;
	cs-gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
      	dmas = <&dma 6>, <&dma 7>;
	dma-names = "tx", "rx";
        status = "okay";

        /delete-node/ spidev@0;
      	/delete-node/ spidev@1;

	touchscreen: tinyft8@0{
                compatible = "ftdi,ft8xx";
      	        reg = <0>;
                spi-max-frequency = <125000000>;
                interrupt-parent = <&gpio>;
		interrupts = <7 IRQ_TYPE_EDGE_FALLING>;
		pinctrl-names = "default";
		pinctrl-0 = <&tft_int_pin>;
		irq-gpios = <&gpio 7 GPIO_ACTIVE_HIGH>;
                touchscreen-size-x = <320>;
                touchscreen-size-y = <240>;
                status = "okay";
        };

};

&spi1 {
	pinctrl-names = "default";
	pinctrl-0 = <&adc_spi_pins &adc_cs_pin>;
	cs-gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
	interrupts = <1 29>;
	status = "okay";
	spidev@0 {
		compatible = "spidev";
		reg = <0>;
		#address-cells = <1>;
		#size-cells = <0>;

		spi-max-frequency = <500000>;
		status = "okay";
	};
};

&spi2 {
	pinctrl-names = "default";
	pinctrl-0 = <&dac_spi_pins &dac_cs_pin>;
	cs-gpios = <&gpio 43 GPIO_ACTIVE_LOW>;
	interrupts = <1 29>;
	status = "okay";
	spidev@0 {
		compatible = "spidev";
		reg = <0>;
		#address-cells = <1>;
		#size-cells = <0>;
		spi-max-frequency = <500000>;
		status = "okay";
	};
};

&pwm {
        status = "disabled";
};

&hdmi {
	hpd-gpios = <&expgpio 0 GPIO_ACTIVE_LOW>;
	status = "okay";
};

&i2c0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&codec_i2c_pins>;
	clock-frequency = <300000>;

	ak4558@13 {
	        #sound-dai-cells = <0>;
		compatible = "akm,ak4558";
                reg = <0x13>;
		pdn-gpio = <&gpio 2 GPIO_ACTIVE_HIGH>;
		status = "okay";
	};
};
	
&i2c1 {
	status = "disabled";
};

&i2c2 {
	status = "okay";
};

&i2s {
	pinctrl-names = "default";
	pinctrl-0 = <&codec_i2s_pins>;
	#sound-dai-cells = <0>;
	clocks = <&clocks BCM2835_CLOCK_PCM>;
	status = "okay";
};

&v3d {
	status = "okay";
};

&vc4 {
	status = "okay";
};

&vec {
	status = "okay";
};

&clocks {
        claim-clocks = <
                     BCM2835_PLLD_DSI0
		     BCM2835_PLLD_DSI1
                     BCM2835_PLLH_AUX
                     BCM2835_PLLH_PIX
                     >;
};
